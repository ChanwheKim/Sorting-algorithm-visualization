<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width">
    <link href='http://fonts.googleapis.com/css?family=Lato:100,300,400,300italic' rel='stylesheet' type='text/css'>
    <title><%= htmlWebpackPlugin.options.title %></title>
</head>
<body>
    <div class="raw">
        <div class="content">
            <h1>Sorting visualization</h1>
        </div>

        <div class="input-container">
            <div class="menu">
                <svg class="menu-icon">
                    <use xlink:href="/assets/images/sprite.svg#icon-menu"></use>
                    <div class="selection-sort">
                        <div class="sort-type">Bubble</div>
                        <div class="sort-type">Insertion</div>
                        <div class="sort-type">Selection</div>
                        <div class="sort-type">Merge</div>
                        <div class="sort-type">Quick</div>
                    </div>
                </svg>
            </div>
            
            <input type="text" class="add-number" placeholder="Please input numbers separated by a comma.">
            <div class="warning-message">test</div>
            
            <button class="btn-run">
                <svg class="sort-icon">
                    <use xlink:href="/assets/images/sprite.svg#icon-sort"></use>
                </svg>
                sort!
            </button>
        </div>

        <div class="screen">
            <div class="sort-label">Sort Label</div>
        </div>
    </div>
</body>
</html>
